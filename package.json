{
  "name": "express-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dayjs": "^1.8.16",
    "debug": "^2.6.9",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jade": "^1.11.0",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"nodemon -r esm server/server.js\"",
    "dev": "set debug=true && npm start",
    "build": "react-scripts build && mv build server",
    "postinstall": "cd server&&npm install",
    "test": "react-scripts test --env=jsdom",
    "format": "prettier --write \"src/**/*.{js,css}\"",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css,md,scss,sass,jsx}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "proxy": "http://localhost:3001"
}
