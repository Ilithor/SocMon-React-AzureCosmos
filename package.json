{
  "name": "express-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dayjs": "^1.8.19",
    "debug": "^4.1.1",
    "eslint-plugin-jsdoc": "^21.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jade": "^1.11.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.11",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "react-scripts": "^3.3.1"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"npm run start:server\"",
    "start:server": "nodemon --watch server -r esm server/server.js",
    "dev": "set debug=true && npm start",
    "build": "react-scripts build && mv build server",
    "postinstall": "cd server&&npm install",
    "test": "react-scripts test --env=jsdom",
    "format": "prettier --write \"src/**/*.{js,css}\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "jsdoc"
    ],
    "extends": [
      "react-app",
      "plugin:jsdoc/recommended"
    ],
    "rules": {
      "jsdoc/require-returns-description": 0
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css,md,scss,sass,jsx}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "proxy": "http://localhost:3001",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
