  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jade": "^1.11.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.3",
    "morgan": "^1.9.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.19.3",
